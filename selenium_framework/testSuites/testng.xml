<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel = "tests" name="Test NG Suite">
<Listeners>
      <Listener class-name = "selenium_framework.Testcomponent.Listeners"/>
</Listeners>
  <test thread-count="5" name="Submit Order Test">
      <classes>        
        <class name="com.selenium_framework.Submitordertest"/>        
      </classes>
  </test> 

  <test thread-count="5" name="ErrorValidation">
      <classes>        
        <class name="com.selenium_framework.ErrorValidation"/>
      </classes>
  </test> 
</suite>